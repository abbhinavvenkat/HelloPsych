$(document).ready(function () {
		$('#select-form img').click(function() {
			value1 = $(this).attr('data-value');
			$('#image-value').attr("value", value1);
			$('#select-form img').removeClass('highlighted');
			$(this).addClass('highlighted');
			});
		});
</script>
